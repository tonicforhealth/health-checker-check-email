{
    "name": "tonicforhealth/health-checker-check-email",
    "description": "Emails checker for health-checker",
    "license": "MIT",
    "keywords": ["tonicforhealth", "health-checker-check-email"],
    "authors": [
        {
            "name": "Dmitry Gopkalo",
            "email": "drefixs@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6",
        "swiftmailer/swiftmailer": "^5.0",
        "php-imap/php-imap": "^2.0",
        "tonicforhealth/health-checker-check": "^0.2.0",
        "zendframework/zenddiagnostics": "^1.0",
        "psr/log": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8|^5.3"
    },
    "autoload": {
        "psr-4": {
            "TonicHealthCheck\\Check\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TonicHealthCheck\\Tests\\Check\\": "tests/"
        }
    },
    "config": {
        "bin-dir": "bin"
    }
}
